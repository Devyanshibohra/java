public class Main{

public static void main(String[] args) {
	
	class Human {
		String name;
		int age;
		double weight;

		Human(String name, int age, double weight){
			this.name=name;
			this.age=age;
			this.weight=weight;
			
			
		}
		void eat() {
			System.out.println(this.name+"is eating");
		}
		void drink() {
			System.out.println(this.name+"is drinking");
		}
		}

	Human human1 = new Human("dolly",19,60);
	Human human2 = new Human("adi",19,70);
	
	human1.eat();
	human2.drink();
	
	
	
}
}
