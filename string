import java.util.ArrayList;

class string {

public static void String(String[] args) {
	
	ArrayList<String> food = new ArrayList<String>();
	food.add("pizza");
	food.add("hamburger");
	food.add("burger");
	
	food.set(0, "sushi");
	food.remove(2);
	//food.clear();

	for(int i=0;i<food.size();i++) {
		System.out.println(food.get(i));
	}
}

}
